{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/circulating-supply",
        "headers": {
          "Cache-Control": "public, max-age=60, stale-while-revalidate=30"
        },
        "dest": "/api/circulating-supply.js"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  